{
	"server": {
		"port": <port-#>,
		"ip": "<ip address>"
	},
	"logs": {
        "logfile": "<full path to log file>",
        "maxsize": 500,
        "maxage": 30,
        "maxbackups": 28,
		"debug": false,
		"info": true,
		"response-ok": true,
		"response-error": true
	},
	"api": {
		"prefix": "/api/v1/",
		"auth": "<secret>",
		"acl": ["127.0.0.0/16", "<network/cidr>"],
		"cmds": {
			"blue": "<full path of the script for blue>",
			"green": "<full path of the script for green>"
		}
	}
}
